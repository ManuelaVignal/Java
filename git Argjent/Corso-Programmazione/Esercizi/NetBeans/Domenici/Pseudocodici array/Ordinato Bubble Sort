//ORDINA BUBBLE SORT

start
	numeri = leggi input;
	
	do 
		ordinato = true;
		indice = 0;
		while(indice < numeri.length -1)do
			if(numeri[indice]>numeri[indice+1])then
				tmp = numeri[indice];
				numeri[indice]=numeri[indice+1];
				numeri[indice+1]=tmp;
				ordinato = false;
			end if
			indice = indice + 1;
		end while
	while(!ordinato)



end

